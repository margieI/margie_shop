    <!DOCTYPE html>
<html>
<head>
    <title>Margie Shop</title>
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" rel="stylesheet"> 

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>

    <script src="//code.jquery.com/jquery-1.11.2.min.js"></script>  
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>


    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
</head>


<body>  	    
  <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">   
    <h2 class="header" id="title-logo">WATER SOUL</h2>

    <div class="nav container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"data-target="#navbar" aria-expanded="false" aria-controls="navbar">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      </div>      
  

      <div id="navbar" class="navbar-collapse collapse">
        <ul id="nav_links" class="nav navbar-nav"> 
          
          <li>
            <%= link_to "HOME", static_pages_landing_page_path %> <!--<a href="#">HOME</a>-->
          </li>

          <li>
            <%= link_to "ABOUT", static_pages_about_path %> <!--<a href="#about">ABOUT</a>-->
          </li>
          
          <li>
            <%= link_to "WORK", static_pages_work_path %> <!--<a href="#work">WORK</a>-->
          </li>

          <li>
            <%= link_to "CONTACT", static_pages_contact_path %><!--<a href="#contact">CONTACT</a>-->         
          </li>

          <li>
            <%= link_to "FAQs", static_pages_faqs_path %><!--<a href="#faqs">FAQS</a>-->         
          </li>
          <div class="search" id="Search">
          <%= form_tag("/products", method: "get") do %>
          <%= label_tag(:q, "Search for:") %>
          <%= text_field_tag(:q) %>
          <%= submit_tag("Search") %>
          <% end %>
        </div>
        </ul>
      </div><!--end navbar-collapse collapse-->
    </div><!--end nav container-->
  </nav><!--end nav-->
<!--<h1>Welcome in the biggest online travel agency</h1>-->
<p class ="notice"><%= notice %></p>
<p class ="alert"><%= alert %></p>


      <ul class="hmenu">
        <%= render 'layouts/registration_items' %>
        <%= render 'layouts/login_items' %>
      </ul>
      <%= yield %>
</body>
</html>
